    "chart.js": "~2.9.3",
    "chartjs-chart-treemap": "~0.2.3",
    "chartjs-plugin-datalabels": "~0.7.0",
    "concurrently": "^5.2.0",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "~2.8.5",
    "custom-env": "~2.0.1",
    "darkmode-js": "^1.5.7",
